{
  "name": "authorsreport",
  "version": "0.0.1",
  "description": "Authors Management Fiori App",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@sap/cds-dk": ">=8",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^4.0.16",
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "ui5-task-zipper": "^3.1.3",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "build:mta": "rimraf resources mta_archives && mbt build",
    "deploy": "fiori cfDeploy",
    "undeploy": "cf undeploy authorsreport --delete-services --delete-service-keys --delete-service-brokers",
    "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml",
    "build-local": "ui5 build preload --clean-dest",
    "start": "ui5 serve"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  }
}
